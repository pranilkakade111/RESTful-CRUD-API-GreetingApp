{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "comma-dangle": 0,
        "no-unused-vars": 0,
        "no-console": 0,
        "global-require": 0,
        "linebreak-style": 0


    }
}
